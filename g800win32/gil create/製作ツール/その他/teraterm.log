10POKE &HD0,&HCD,&H53,&HBE,&H32,&HF0,&H00,&HC9:N=0:B=1:X=0:Y=0
20CALL &HD0:K= PEEK &HF0:K=K-&H1E
30ON K GOTO *DOWN,*UP,*LEFT,*RIGHT
35CLS :GOTO 20
40*DOWN:LOCATE 0,0:PRINT "DOWN ":GCURSOR (X,Y):IF B=0 GPRINT "C0CCFFDFCEC6":B=1 ELSE GPRINT "C0CCDFFFCEC6":B=0
45Y=Y+1:GOTO *TIME
50*UP
60LOCATE 0,0:PRINT "UP   ":GCURSOR (X,Y):IF B=0 GPRINT "G6CEDFFFCCC0":B=1 ELSE GPRINT "C6CEFFDFCCC0":B=0
65Y=Y-1:GOTO *TIME
70*RIGHT
80LOCATE 0,0:PRINT "RIGHT":GCURSOR (X,Y):IF B=0 GPRINT "C0E0DFDFE6C0":B=1 ELSE GPRINT "C0C0FFFFC6C0":B=0
85X=X+1:GOTO *TIME
90*LEFT
100LOCATE 0,0:PRINT "LEFT ":GCURSOR (X,Y):IF B=0 GPRINT "C0E6DFDFE0C0":B=1 ELSE GPRINT "C0C6FFFFC0C0":B=0
105X=X-1:GOTO *TIME
110CLS
120GOTO 20
1000*TIME
1010FOR I=0 TO 15:NEXT :GOTO 20
